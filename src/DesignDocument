    GameRunner


    Enemy
        private int xCoord
        private int yCoord

    Player
        private int xCoord
        private int yCoord

        public int getXCoord()
            returns value stored in xCoord instance variable

        public int getYCoord()
            returns value stored in yCoord instance variable
